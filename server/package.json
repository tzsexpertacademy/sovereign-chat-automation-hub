{
  "name": "whatsapp-multi-client-backend",
  "version": "1.0.0",
  "description": "Backend para sistema WhatsApp Multi-Cliente",
  "main": "whatsapp-multi-client-server.js",
  "scripts": {
    "start": "../scripts/production-start-whatsapp.sh",
    "dev": "PORT=4001 node whatsapp-multi-client-server.js",
    "dev-watch": "PORT=4001 nodemon whatsapp-multi-client-server.js",
    "stop": "../scripts/production-stop-whatsapp.sh",
    "restart": "npm run stop && sleep 2 && npm run start",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "force-stop": "../scripts/force-stop-whatsapp.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "puppeteer": "^21.0.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "1.24.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "whatsapp",
    "multi-client",
    "saas",
    "nodejs"
  ],
  "author": "TZS Expert Academy",
  "license": "MIT"
}
